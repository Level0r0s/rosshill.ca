{
  "name": "homepage",
  "version": "0.0.1",
  "scripts": {
    "dev": "sapper dev",
    "build": "npm run blogGen && sapper build --legacy",
    "export": "npm run blogGen && sapper export --legacy",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run",
    "blogGen": "node node_modules/frontmatter-markdown-to-json --in=content --out=content"
  },
  "dependencies": {
    "compression": "^1.7.1",
    "dateformat": "^3.0.3",
    "polka": "^0.4.0",
    "sirv": "^0.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "chokidar": "^2.0.4",
    "frontmatter-markdown-to-json": "0.0.9",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.0.0",
    "rollup-plugin-babel": "^4.0.2",
    "rollup-plugin-commonjs": "^9.1.6",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-svelte": "^5.0.1",
    "rollup-plugin-terser": "^1.0.1",
    "sapper": "^0.24.2",
    "slugify": "^1.3.4",
    "svelte": "^2.0.0",
    "xmldom": "^0.1.27"
  }
}
